{
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-beta.1"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.12",
    "html-minifier": "^4.0.0",
    "lightningcss-cli": "^1.15.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.9"
  },
  "scripts": {
    "watch:sass": "sass  --no-source-map --watch style:_site/style",
    "watch:eleventy": "eleventy --serve",
    "build:rm": "rm -rf _site",
    "build:sass": "sass  --no-source-map style:_site/style",
    "build:minify_css": "lightningcss --minify --targets '> 0.25%, not IE 11' _site/style/*.css -o _site/style/*.css",
    "build:eleventy": "eleventy",
    "build:minify_html": "html-minifier --input-dir _site --output-dir _site --collapse-whitespace --file-ext html",
    "remove": "bun run build:rm",
    "build_sass": "bun run build:sass",
    "minify_css": "bun run build:minify_css",
    "build_eleventy": "bun run build:eleventy",
    "minify_html": "build:minify_html"
  },
  "browserslist": [
    "> 0.25%",
    "not IE 11"
  ]
}